{
  "name": "client",
  "version": "0.0.1",
  "scripts": {
    "ng": "./node_modules/@angular/cli/bin/ng",
    "start": "ng serve --port 4200 --disableHostCheck --proxy-config proxy.conf.json --base-href /",
    "build": "rm -f ./dist/client/*.gz && ng build --prod --aot",
    "compress": "node ./node_modules/gzipper/bin/index.js c ./dist --verbose",
    "stats": "ng build --stats-json",
    "analyze": "node ./node_modules/webpack-bundle-analyzer/lib/bin/analyzer.js -h 0.0.0.0 -p 4200 ./dist/client/stats-es2015.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.8",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "^11.0.8",
    "@angular/compiler": "^11.0.8",
    "@angular/core": "^11.0.8",
    "@angular/forms": "^11.0.8",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "^11.0.8",
    "@angular/platform-browser-dynamic": "^11.0.8",
    "@angular/router": "^11.0.8",
    "@types/lodash": "^4.14.150",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular/cli": "^11.0.6",
    "@angular/compiler-cli": "^11.0.8",
    "@angular/language-service": "^11.0.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.9.5",
    "codelyzer": "^6.0.0",
    "gzipper": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
