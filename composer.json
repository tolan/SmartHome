{
    "name": "tolan/smart-home",
    "version": "0.0.1",
    "type": "app",
    "license": "MIT",
    "prefer-stable": true,
    "sort-packages": true,
    "authors": [
        {
            "name": "Martin Kovar",
            "email": "mkovar86@gmail.com"
        }
    ],
    "scripts": {
        "cs": "./vendor/bin/phpcs -s",
        "unit": "./vendor/bin/phpunit",
        "migrate": "./vendor/bin/doctrine-migrations",
        "coverage": "mkdir -p output && ./vendor/bin/phpunit --coverage-html output"
    },
    "require": {
        "php": ">=7.3.0",
        "slim/http": "^1.2.0",
        "slim/psr7": "^1.3.0",
        "slim/slim": "^4.7.1",
        "php-di/php-di": "^6.3",
        "monolog/monolog": "^2.2",
        "ducks-project/spl-types": "v1.1.0",
        "doctrine/orm": "^2.8.5",
        "bryanjhv/slim-session": "^4.1",
        "bluerhinos/phpmqtt": "1.0.0",
        "doctrine/annotations": "^1.12.1",
        "guzzlehttp/guzzle": "^7.2",
        "laminas/laminas-http": "^2.14.3",
        "symfony/process": "^5.2",
        "selective/samesite-cookie": "^0.1.0",
        "seld/signal-handler": "^1.2",
        "predis/predis": "^1.1",
        "psr/simple-cache": "^1.0",
        "doctrine/cache": "^1.10",
        "doctrine/mongodb-odm": "^2.2",
        "geerlingguy/ping": "^1.2",
        "mtdowling/jmespath.php": "^2.6",
        "doctrine/migrations": "^3.1",
        "psr/cache": "1.0.1",
        "elasticsearch/elasticsearch": "^7.13",
        "wrench/wrench": "v3.0.0-rc1"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "SmartHome\\": "server",
            "SmartHome\\Unit\\": "tests\\Unit"
        }
    }
}
