        "bryanjhv/slim-session": "^4.0.0",
        "bluerhinos/phpmqtt": "1.0.0",
        "doctrine/annotations": "^1.10",
        "guzzlehttp/guzzle": "^6.5",
        "laminas/laminas-http": "^2.11",
        "symfony/process": "^5.0",
        "selective/samesite-cookie": "^0.1.0",
        "seld/signal-handler": "^1.2",
        "predis/predis": "^1.1",
        "psr/simple-cache": "^1.0",
        "doctrine/cache": "^1.10"
